Scanner sc=new Scanner(System.in);
		int num1=sc.nextInt();
		
		int arr[][]=new int[num1][num1];
		int number=1;
		
		for(int k=0;k<arr.length;k++) {
			for(int i=0;i<num1;i++) {
				arr[i][k]=number;
				number++;
			}
		}
		for(int k=0;k<arr.length;k++) {
			for(int i=0;i<num1;i++) {
				System.out.print(arr[k][i]+" ");
			}
			System.out.println();
		}
