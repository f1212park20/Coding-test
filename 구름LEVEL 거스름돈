
#백준 사이트 코드를 이용해서 제작

n = 1000-int(input())
count = 0
coin_type = [500,100,50,10]#리스트로 만들기

b=[]#b를 초가화
for i in  coin_type:
    count= n//i#1,0,1,2
    n=n%i#나머지값 계산
    b.append(count)#b에 count값 추가 
    new_line = list(map(int,b)) # 리스트  ->  정수(int) 
print(new_line[0],new_line[1],new_line[2],new_line[3],end=' ')#결과 값 출력
